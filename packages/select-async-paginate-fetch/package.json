{
  "name": "select-async-paginate-fetch",
  "version": "1.0.0-alpha.1",
  "description": "Wrapper above select-async-paginate-model that loads options by specified url",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "typings": "./dist/index.d.ts",
  "keywords": [
    "react",
    "react-select",
    "async",
    "pagination",
    "fetch"
  ],
  "repository": "git@github.com:vtaits/react-select-async-paginate.git",
  "homepage": "https://github.com/vtaits/react-select-async-paginate/tree/master/packages/select-async-paginate-model",
  "author": "Vadim Taits",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "biome check src",
    "lint:fix": "biome check src --write --unsafe",
    "format": "biome format src --write",
    "prepare": "bun run build",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest run --coverage.enabled --coverage.all --coverage.include=src",
    "test:unit:watch": "vitest",
    "codecov": "codecov -F select_async_paginate_fetch"
  },
  "dependencies": {
    "krustykrab": "^1.1.0",
    "qs": "^6.14.0",
    "select-async-paginate-model": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/qs": "^6.9.18",
    "@vitest/coverage-v8": "^3.0.5",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5",
    "vitest-fetch-mock": "^0.4.3"
  }
}
